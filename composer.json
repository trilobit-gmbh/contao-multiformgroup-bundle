{
    "name": "trilobit-gmbh/contao-multiformgroup-bundle",
    "type": "contao-bundle",
    "description": "Allow dynamic repetition of form groups",
    "keywords": [
        "contao",
        "forms",
        "multiple",
        "repetition",
        "form generator"
    ],
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "trilobit GmbH",
            "homepage": "https://github.com/trilobit-gmbh"
        }
    ],
    "require": {
        "php": ">=7.0",
        "contao/core-bundle": "~4.4"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.1"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Trilobit\\ContaoMultiFormGroup\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "Trilobit\\ContaoMultiFormGroup\\ContaoManager\\Plugin"
    }
}
